{
  "name": "fbi-warning",
  "version": "1.0.0",
  "description": "Node.js 种子神器",
  "main": "dist/index.js",
  "homepage": "https://github.com/nusr/FBIWarning",
  "author": "Steve Xu stevexugc@gmail.com",
  "repository": "https://github.com/nusr/FBIWarning.git",
  "scripts": {
    "dev": "debug_mode=1 node ./dist/index.js http://127.0.0.1:1087",
    "build": "npm run build-ts && npm run lint",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "bluebird": "^3.5.4",
    "cheerio": "^1.0.0-rc.3",
    "global-tunnel-ng": "^2.7.1",
    "iconv-lite": "^0.4.24",
    "request": "^2.88.0",
    "socks5-http-client": "^1.0.4",
    "socks5-https-client": "^1.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/cheerio": "^0.22.11",
    "@types/global-tunnel-ng": "^2.1.0",
    "@types/node": "^11.13.0",
    "@types/request": "^2.48.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "lint-staged": {
    "linters": {
      "**/*.{ts,tsx}": [
        "tslint -c tslint.json -p tsconfig.json"
      ]
    },
    "ignore": [
      "**/dist/*",
      "**/result/**/*",
      "**/json/*"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "keywords": [
    "downloader",
    "Node.js",
    "FBI Warning",
    "Spider"
  ],
  "bin": {
    "fbi-warning": "dist/index.js"
  },
  "license": "MIT"
}

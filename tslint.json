{
  "rules": {
    "no-arg": true,
    "no-construct": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-sparse-arrays": true,
    "no-string-throw": true,
    "no-unsafe-finally": true,
    "no-var-keyword": true,
    "number-literal-format": true,
    "curly": true,
    "class-name": true,
    "label-position": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true
  },
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
      "dist/*.js",
      "node_modules/**/*.ts"
    ]
  }
}